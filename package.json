{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-css": "node-sass --include-path scss resources/sass/app.scss   public/css/app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.4.3",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.1",
    "v-dialogs": "^2.1.4",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.5",
    "vue-js-modal": "^2.0.0-rc.3",
    "vue-router": "^3.1.6",
    "vue-smooth-scrollbar": "^0.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
